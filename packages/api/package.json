{
  "name": "private-notes-api",
  "version": "0.1.0",
  "author": "Miitri Raatikainen",
  "description": "Private Notes api specification",
  "private": true,
  "scripts": {
    "build": "pnpm build:api && pnpm build:docs",
    "build:api": "pnpm openapi-typescript private-notes-api.yaml --output dist/private-notes-api.d.ts",
    "build:docs": "pnpm redoc-cli build private-notes-api.yaml",
    "deploy:dev": "pnpm build && node scripts/deploy.js --env dev"
  },
  "dependencies": {
    "openapi-typescript": "^6.1.0",
    "redoc-cli": "^0.13.20"
  }
}
